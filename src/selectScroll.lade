div.latte-selectScroll (data:"selectScroll")
	div.latte-input
		div.latte-input-icon.latte-icon-caret-top (click:"selectOpen")
		input.latte-input-inner (duplex:"selectValue")
	div.latte-selectScroll-wrapper (show:"{{show}}")
		div.latte-selectScroll-wrap (scroll:"scroll")
			div.latte-selectScroll-list (list:"selects")
				div.latte-selectScroll-item.li (click:"click")
					latte  "{{key}}"
			div.latte-selectScroll-cover